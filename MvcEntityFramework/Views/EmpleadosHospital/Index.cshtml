@model List<EmpleadoHospital>

<a asp-action="ProcedimientoEmpleadosHospital" asp-controller="EmpleadosHospital">
    Procedimiento Out
</a>
<table class="table table-bordered">
    <tr>
        <th>Apellido</th>
        <th>Salario</th>
        <th>ID Empleado</th>
    </tr>
    @foreach(EmpleadoHospital emp in Model)
    {
        <tr>
            <td>@emp.Apellido</td>
            <td>@emp.Salario</td>
            <td>@emp.IdEmpleado</td>
        </tr>
    }
</table>