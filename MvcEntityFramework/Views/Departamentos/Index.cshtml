@model List<Departamento>

<h1>DI ACCESO A DATOS</h1>
<table>
    <thead>
        <tr>
            <th>Número</th>
            <th>Nombre</th>
            <th>Localidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Departamento dept in Model)
        {
            <tr>
                <td>@dept.Numero</td>
                <td>@dept.Nombre</td>
                <td>@dept.Localidad</td>
            </tr>
        }
    </tbody>
</table>